        "yup": "^0.32.9"
    },
    "scripts": {
        "start": "node --expose-gc --max-old-space-size=8192 node_modules/react-scripts/scripts/start.js",
        "build": "node --expose-gc --max-old-space-size=8192 node_modules/react-scripts/scripts/build.js",
        "test": "node --expose-gc --max-old-space-size=8192 node_modules/react-scripts/scripts/test.js",
        "eject": "node --expose-gc --max-old-space-size=8192 node_modules/react-scripts/scripts/eject.js",
        "lint": "eslint ./src",
        "lint-fix": "eslint ./src --fix",
        "depcheck": "depcheck .",
        "eslint-check": "eslint --print-config path/to/main.js | eslint-config-prettier-check",
        "precommit": "echo \"Frontend: running pre-commit checks\" && npm run lint"
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es2020": true
        },
        "extends": [
            "react-app",
            "plugin:prettier/recommended"
        ],
        "plugins": [
            "react",
            "prettier"
        ],
        "parserOptions": {
            "ecmaVersion": 2020
        },
        "rules": {
            "prettier/prettier": [
                "error",
                {
                    "printWidth": 80,
                    "trailingComma": "es5",
                    "semi": false,
                    "jsxSingleQuote": false,
                    "singleQuote": true,
                    "useTabs": false,
                    "tabWidth": 4,
                    "bracketSpacing": true,
                    "arrowParens": "avoid"
                }
            ],
            "no-console": 0,
            "jsx-a11y/img-redundant-alt": 0
        }
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "proxy": "http://localhost:2222",
    "engines": {
        "node": "^16.3.0",
        "npm": "^7.15.0"
    },
    "devDependencies": {
        "depcheck": "^1.4.1",
        "eslint": "^7.28.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.4.0",
        "prettier": "^2.3.1"
    }
}
